<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.errata.impl.UnpublishedKeyword"
        table="rhnErrataKeywordTmp" >

        <composite-id unsaved-value="any">
            <key-many-to-one name="errata"
                class="com.redhat.rhn.domain.errata.impl.UnpublishedErrata"
                column="errata_id"/>
            <key-property name="keyword" column="keyword"
                type="string" length="64"/>
        </composite-id>

        <property name="created" column="created" type="timestamp"
            insert="false" update="false"/>
        <property name="modified" column="modified" type="timestamp"
            insert="false" update="false"/>
    </class>
</hibernate-mapping>
