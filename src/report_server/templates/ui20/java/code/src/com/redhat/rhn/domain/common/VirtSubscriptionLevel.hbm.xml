<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.common.VirtSubscriptionLevel"
        table="rhnVirtSublevel" mutable="false">
        <cache usage="read-only"/>
        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="assigned"/>
        </id>
        <property name="label" column="LABEL" not-null="true" type="string"
            length="32"/>
        <property name="name" column="NAME" not-null="true" type="string"
            length="128"/>
        <property name="created" column="CREATED" not-null="true" type="timestamp"
            insert="false" update="false"/>
        <property name="modified" column="MODIFIED" not-null="true" type="timestamp"
            insert="false" update="false"/>
    </class>
</hibernate-mapping>