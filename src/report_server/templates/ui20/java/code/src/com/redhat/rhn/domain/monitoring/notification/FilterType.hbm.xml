<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.monitoring.notification.FilterType"
        table="rhn_redirect_types" >
        <id name="name" type="string" column="name">
            <meta attribute="scope-set">protected</meta>
            <generator class="assigned"/>
        </id>
        <property name="description" column="description"  type="string" length="255" />
        <property name="longName" column="long_name"  type="string" length="80" />
    </class>
</hibernate-mapping>
